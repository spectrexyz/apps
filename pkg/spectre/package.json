{
  "name": "spectre",
  "private": true,
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build --mode production && cp netlify_redirects dist/_redirects",
    "serve": "vite preview",
    "import:moire-assets": "pnpm moire-copy-assets ./public/moire",
    "lint": "eslint --ext .tsx,.ts src/",
    "generate:subgraph-code": "cp spectre-subgraph/schema-base.graphql spectre-subgraph/schema.graphql && cat node_modules/spectre/schema.graphql >> spectre-subgraph/schema.graphql && graphql-codegen --config ./spectre-subgraph/codegen.yml && dprint fmt ./src/spectre-subgraph.ts"
  },
  "dependencies": {
    "@bpierre/use-viewport": "^0.3.0",
    "@elchininet/isometric": "^3.6.0",
    "@emotion/cache": "^11.10.1",
    "@emotion/react": "^11.10.0",
    "@ngneat/falso": "^6.1.0",
    "@tanstack/react-query": "^4.2.1",
    "@types/lodash.uniqby": "^4.7.7",
    "buffer": "^6.0.3",
    "csstype": "^3.1.0",
    "dnum": "^1.5.1",
    "ethers": "^5.7.0",
    "events": "^3.3.0",
    "graphql": "^16.6.0",
    "lodash.uniqby": "^4.7.0",
    "markdown-to-jsx": "^7.1.7",
    "moire": "workspace:*",
    "ms": "^3.0.0-canary.1",
    "nft.storage": "^7.0.0",
    "process": "^0.11.10",
    "rand-seed": "^1.0.1",
    "react": "^18.2.0",
    "react-cool-inview": "^3.0.1",
    "react-dom": "^18.2.0",
    "react-spring": "^9.5.2",
    "stylis": "^4.1.1",
    "use-nft": "^0.12.0",
    "wagmi": "^0.6.4",
    "zod": "^3.18.0",
    "zustand": "4.1.0"
  },
  "devDependencies": {
    "@ethersproject/providers": "^5.7.0",
    "@graphql-codegen/add": "^3.2.1",
    "@graphql-codegen/cli": "2.11.6",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-document-nodes": "2.3.3",
    "@graphql-codegen/typescript-graphql-files-modules": "2.2.1",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@graphql-codegen/typescript-react-query": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@vitejs/plugin-react": "^2.0.1",
    "eslint": "^8.22.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "graphql-codegen-typescript-validation-schema": "^0.6.0",
    "spectre": "github:spectrexyz/subgraph",
    "typescript": "^4.7.4",
    "vite": "^3.0.9",
    "vite-plugin-checker": "^0.4.9",
    "vite-plugin-rewrite-all": "^1.0.0"
  }
}
