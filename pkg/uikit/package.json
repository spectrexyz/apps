{
  "name": "uikit",
  "private": true,
  "main": "dist/uikit.cjs.js",
  "module": "dist/uikit.esm.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "bin": {
    "uikit-copy-assets": "./bin/copy-assets"
  },
  "scripts": {
    "build": "yarn export-assets && rollpkg build --noStats",
    "dev": "yarn export-assets && rollpkg watch",
    "lint": "eslint src",
    "export-assets": "./scripts/export-assets.sh",
    "prepublishOnly": "yarn build"
  },
  "peerDependencies": {
    "@emotion/cache": "11.x",
    "@emotion/react": "11.x",
    "react": "17.x",
    "react-dom": "17.x",
    "react-spring": "9.x",
    "stylis": "2.x"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/rollpkg/configs/eslint"
    ]
  },
  "jest": {
    "preset": "rollpkg"
  },
  "devDependencies": {
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollpkg": "^0.5.5",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "react-use-measure": "^2.0.4"
  }
}
